local library = loadstring(game:HttpGet("https://pastebin.com/raw/GAGA5H8t"))();

local Gamer = library:CreateSection("King Piece");
Gamer:Label("~ Press Right Shift To Hide GUI ~");
Gamer:Button("Rems Hub", function()
loadstring(game:HttpGet("https://remcc.xyz/amogus.lua"))();
end)
Gamer:Button("Mhee Hub", function()
loadstring(game:HttpGet("https://pastebin.com/raw/KyP4n2rT",true))()
end)
Gamer:Button("GUI 3", function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/sannin9000/scripts/main/kinglegacy.lua"))()
end)
Gamer:Button("Devil Fruit Farm", function()
for i,v in pairs(game.Workspace:GetDescendants()) do
   if v:IsA("Tool") and v.Parent == Workspace and v:FindFirstChild("Handle") and v:FindFirstChild("DevilFruit") then
       game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Handle.CFrame
   end
end
end)
Gamer:Label("~ Thanks for using Furk Ultra ~");

library:Ready();
