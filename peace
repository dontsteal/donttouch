local library = loadstring(game:HttpGet("https://pastebin.com/raw/GAGA5H8t"))();

local Gamer = library:CreateSection("King Piece");
Gamer:Label("~ Press Right Shift To Hide GUI ~");
Gamer:Button("Mhee Hub", function()
loadstring(game:HttpGet("https://pastebin.com/raw/KyP4n2rT",true))()
end)
Gamer:Button("GUI 2", function()
_G.RaidFarm = false -- put to "true" if u want to enable Raid Farm
_G.RaidWeapon = "Xmas Blade" -- put ur weapon in if u enable Raid Farm
loadstring(game:HttpGet(('https://raw.githubusercontent.com/AltsegoD/script/master/KingPiece.lua')))()
end)
Gamer:Label("~ Thanks for using Furk Ultra ~");

library:Ready();
